
 &filelist
        base_filename = 'output/nozzle_hd_'
        saveprim      = .true.
        autoconvert   = .true.
        convert_type  = 'vtuBCCmpi'
        nwauxio       = 5
 /


 &savelist
        itsave(1,1) = 0
        itsave(1,2) = 0
        dtsave_log  = 0.05d0
        dtsave_dat  = 0.5d0
 /

 &stoplist
        time_max = 100.0d0
 /

 &methodlist
         time_stepper  = 'threestep'
         flux_scheme   = 20*'hllc'
         limiter       = 20*'cada3'
 /


 &boundlist
        typeboundary_min1 = 3*'special'
        typeboundary_max1 = 3*'special'
 /

 &meshlist
        refine_max_level = 4
        block_nx1        = 10
        domain_nx1       = 180
        xprobmin1        = -3.0d0
        xprobmax1        = 3.0d0
 /


 &paramlist
        typecourant = 'maxsum'
        slowsteps   = 10
        courantpar  = 0.8d0
 /

 &hd_list
        hd_energy     =.true.
        hd_gamma      = 1.666667d0
 /

 &usr_list
        mach0         = 0.1d0
/
